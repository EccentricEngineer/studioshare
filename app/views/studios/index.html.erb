<div>
<%= link_to "Add a new Studio", new_studio_path, class: 'btn btn-dark' %>
</div>
<div class="card-block">
    <%@studios.each do |studio| %>
      <div class="studio-card">
      <div class="card-img" style="background-image: url(https://images.unsplash.com/photo-1598488035139-bdbb2231ce04?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80)">
      </div>
      <div class="studio-card-text">
        <%# <p><%=studio.price %></p>
        <h3><%= link_to studio.name, studio_path(studio) %></h3>
        <h4><%= studio.address %></h4>
      </div>
      </div>
 <% end %>
</div>
